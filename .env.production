# Production Environment Variables
VITE_APP_NAME=AyuTrace
VITE_APP_VERSION=2.0.0
VITE_APP_ENVIRONMENT=production

# API Configuration
VITE_API_BASE_URL=https://ayutrace-backend-api.onrender.com
VITE_API_TIMEOUT=30000
VITE_API_CORS_ENABLED=true

# Features Flags
VITE_FEATURE_BLOCKCHAIN=true
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_NOTIFICATIONS=true
VITE_FEATURE_DEBUG_MODE=false

# Blockchain Configuration
VITE_BLOCKCHAIN_NETWORK=mainnet
VITE_BLOCKCHAIN_CHAIN_ID=1

# UI Configuration
VITE_THEME=light
VITE_ENABLE_ANIMATIONS=true

# Security
VITE_ENABLE_HTTPS=true
VITE_SECURE_COOKIES=true

# Performance
VITE_ENABLE_SW=true
VITE_ENABLE_COMPRESSION=true

# Logging
VITE_LOG_LEVEL=error
VITE_ENABLE_CONSOLE_LOGS=false