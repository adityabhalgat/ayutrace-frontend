# Development Environment Variables
VITE_APP_NAME=AyuTrace
VITE_APP_VERSION=2.0.0
VITE_APP_ENVIRONMENT=development

# API Configuration
VITE_API_BASE_URL=http://localhost:3000
VITE_API_TIMEOUT=10000
VITE_API_CORS_ENABLED=true

# Features Flags
VITE_FEATURE_BLOCKCHAIN=true
VITE_FEATURE_ANALYTICS=true
VITE_FEATURE_NOTIFICATIONS=true
VITE_FEATURE_DEBUG_MODE=true

# Blockchain Configuration
VITE_BLOCKCHAIN_NETWORK=development
VITE_BLOCKCHAIN_CHAIN_ID=32343

# UI Configuration
VITE_THEME=light
VITE_ENABLE_ANIMATIONS=true

# Security
VITE_ENABLE_HTTPS=false
VITE_SECURE_COOKIES=false

# Performance
VITE_ENABLE_SW=false
VITE_ENABLE_COMPRESSION=false

# Logging
VITE_LOG_LEVEL=debug
VITE_ENABLE_CONSOLE_LOGS=true